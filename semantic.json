{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../vendor/semantic",
      "uncompressed": "../vendor/semantic/components/",
      "compressed": "../vendor/semantic/components/",
      "themes": "../vendor/semantic/themes/"
    },
    "clean": "../vendor/semantic"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "button",
    "divider",
    "header",
    "image",
    "input",
    "form",
    "message",
    "statistic",
    "checkbox",
    "dropdown",
    "api",
    "form",
    "visibility"
  ],
  "version": "2.2.14"
}
